<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>DOTDCD - ERP</title>
    {{> include/_head}}
    {{{_sections.head}}}
    {{#>head-block}}{{/head-block}}
</head>
    

<body class="mod-bg-1 mod-nav-link">

    {{> include/_scripts-loading-saving}}

    <div class="page-wrapper">
        <div class="page-inner">
            {{> include/_left-panel }}
            <div class="page-content-wrapper">
                {{> include/_page-header}}
                <main id="js-page-content" role="main" class="page-content">
                    {{> flash/alerts}}
                    <ol class="breadcrumb page-breadcrumb" >
                        <li class="breadcrumb-item">
                            <section id="demo" >
                                <div class="ow-widget">
                                    <div class="ow-row" style="width: 131px !important;">
                                        <span class="ow-city-name" ></span>
                                    </div>
                                    <div class="ow-row">
                                        <div class="ow-temp-current pull-left"></div>
                                        <div class="ow-current-desc pull-left">

                                        </div>
                                    </div>
                                </div>
                            </section>
                        </li>
                        <li class="position-absolute pos-top pos-right d-none d-sm-block"><span
                                class="js-get-date"></span></li>
                    </ol>
                    
                    {{{body}}}
                </main>
                <!-- this overlay is activated only when mobile menu is triggered -->
                <div class="page-content-overlay" data-action="toggle" data-class="mobile-nav-on"></div>

                {{> include/_page-footer}}
                {{> include/_shortcut-modal}}
                {{> include/_color-profile-reference}}

            </div>
        </div>
    </div>
    {{> include/_shortcut-menu}}
    <script src="/js/loader.js"></script>
    {{> include/_page-settings}}
    {{> include/_scripts-base-plugins}}
    {{{_sections.scripts}}}
    
</body>

</html>