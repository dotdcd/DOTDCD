//? genera los cards, con colores de clases de bootstrap
function getPercentajeEdificios() {
  const colors = [    "bg-primary",    "bg-secondary",    "bg-success",    "bg-danger",    "bg-warning",    "bg-info",    "bg-light"  ];
  let index = 0; //? contador para el array de colores
  const row = document.createElement("div");    //? crea la fila
  row.className = "row justify-content-center"; //? tama√±o de la fila
  for (const building of infoEdificios) {
    if (building.Total === 0 || building.Total === "" || building.TotalParking === 0 || building.TotalParking === "") { //? si no hay unidades o estacionamientos, no se muestra
      continue;
    }

    const occupationPercentage = ((building.Occupation / building.Total) * 100).toFixed(0); //? porcentaje de ocupaci√≥n
    const parkingOccupationPercentage = ((building.ParkingOccupation / building.TotalParking) * 100).toFixed(0); //? porcentaje de ocupaci√≥n de estacionamiento

    const card = document.createElement("div"); //? crea la card
    card.className = "col-3"; //? tama√±o de la card
    card.innerHTML = `
      <div class="p-3 ${colors[index]} rounded shadow-lg overflow-hidden position-relative  text-dark mb-g">
        <h1 class="display-4 font-weight-bold" id="titulo2">${building.EdificioName}</h1>
        <div class="">
          <h3 class="display-4 d-block l-h-n m-0 fw-500">
            <div class="row  mb-3">
              <div class="col-6"><i class="fal fa-light fa-city"></i> <span>${occupationPercentage}%</span></div> 
              <div class="col-6"><i class="fal fa-light fa-square-parking"></i> <span>${parkingOccupationPercentage}%</span></div>
            </div>
            <div class="row">
              <div class="col-6">${building.Total}/${building.Occupation}</div>
              <div class="col-6">${building.TotalParking}/${building.ParkingOccupation}</div>
            </div>
          </h3>
        </div>
      </div>
    `; //? contenido de la card
    row.appendChild(card); //? agrega la card a la fila
    index = (index + 1) % colors.length; //? cambia el color de la card
  }

  document.querySelector("#cardsContainer").appendChild(row); //? agrega la fila a la pagina
}



