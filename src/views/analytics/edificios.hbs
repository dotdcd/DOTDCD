function printEdificio(index) {
  var table = document.createElement("table");
  table.className = "table table-success bordered table-bordered";

  var thead = document.createElement("thead");
  var tr = document.createElement("tr");

  var th = document.createElement("th");
  th.textContent = infoEdificiosGeneral[index].Edificio;
  tr.appendChild(th);

  thead.appendChild(tr);
  table.appendChild(thead);

  var tbody = document.createElement("tbody");

  var limit = 10;
  var count = 0;
  var row;

  infoEdificiosGeneral[index].Deptos.forEach(function(depto) {
    if (count % limit == 0) {
      if (row) {
        tbody.appendChild(row);
      }
      row = document.createElement("tr");
    }

    var td = document.createElement("td");
    var td1 = document.createElement("td");
    var td2 = document.createElement("td");
    td.innerHTML = depto.Unidad;
    td1.innerHTML = depto.Costo;
    td2.innerHTML = depto.Fecha;
    row.appendChild(td);
    row.appendChild(td1);
    row.appendChild(td2);

    count++;

    if (count % 10 == 0) {
      limit = 10;       
    }
  });

  if (row) {
    tbody.appendChild(row);
  }

  table.appendChild(tbody);

  document.querySelector(".col-md-12").appendChild(table);
}
